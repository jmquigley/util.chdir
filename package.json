{
  "name": "util.chdir",
  "version": "0.0.14",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "An implementation of pushd/popd for JavaScript.",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.chdir"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.chdir/issues"
  },
  "engines": {
    "node": ">=7.4"
  },
  "dependencies": {
    "expand-home-dir": "0.0.3",
    "fs-extra": "^2.0.0",
    "util.pkgwrap": "0.0.5",
    "util.rstrip": "0.0.7",
    "util.timestamp": "0.0.13",
    "uuid": "^3.0.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "coveralls": "^2.11.15",
    "nyc": "^10.1.2",
    "xo": "^0.17.1"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/pkgwrap --postinstall",
    "test": "./node_modules/.bin/pkgwrap --testing",
    "report": "./node_modules/.bin/pkgwrap --reporting",
    "coverage": "./node_modules/.bin/pkgwrap --coverage"
  },
  "xo": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "global": [
      "window",
      "describe",
      "it",
      "JSON"
    ],
    "rules": {
      "space-before-function-paren": "off",
      "unicorn/filename-case": [
        "error",
        {
          "case": "snakeCase"
        }
      ],
      "arrow-parens": [
        "error",
        "as-needed"
      ]
    }
  }
}
