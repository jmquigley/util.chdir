{
	"name": "util.chdir",
	"version": "0.0.2",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "An implementation of pushd/popd for JavaScript.",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.chdir"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.chdir/issues"
	},
	"engines": {
		"node": ">=6.5"
	},
	"dependencies": {
		"fs-extra": "^2.0.0"
	},
	"devDependencies": {
		"ava": "^0.17.0",
		"xo": "^0.17.1"
	},
	"main": "index.js",
	"scripts": {
		"test": "xo && ava --verbose"
	},
	"xo": {
		"env": {
			"browser": true,
			"node": true,
			"es6": true
		},
		"global": [
			"window",
			"describe",
			"it",
			"JSON"
		],
		"rules": {
			"space-before-function-paren": "off",
			"unicorn/filename-case": [
				"error",
				{
					"case": "snakeCase"
				}
			],
			"arrow-parens": [
				"error",
				"as-needed"
			]
		}
	}
}
